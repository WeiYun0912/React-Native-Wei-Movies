{
  "expo": {
    "name": "Wei's Movie",
    "slug": "wei-movie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/movie.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/movie.png",
